<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.telecontrol">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/telecontrol_icon"
        android:roundIcon="@mipmap/telecontrol_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.telecontrol.App.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.telecontrol.ActivityClass.Setting_UI"
            android:label="设置"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.telecontrol.ActivityClass.Network_details"
            android:label="网络信息"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.telecontrol.ActivityClass.Chart_details"
            android:label="详细信息"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>